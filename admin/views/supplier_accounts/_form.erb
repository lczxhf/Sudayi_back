<% error = @supplier_account.errors.include?(:supplier_address_id) %>
<fieldset class='control-group <%= error ? 'has-error' : ''%>'>
  <%= f.label :supplier_address_id, :class => 'control-label' %>
  <div class='controls'>
    <%= f.text_field :supplier_address_id, :class => 'form-control input-xlarge input-with-feedback', :autofocus => true %>
    <span class='help-inline'><%= error ? f.error_message_on(:supplier_address_id) : "Ex: a simple text" %></span>
  </div>
</fieldset>
