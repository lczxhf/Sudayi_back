<ul class="nav nav-tabs">
  <li> <%= link_to tag_icon(:list, pat(:list)), url(:store_products, :index) %></li>
  <li> <%= link_to tag_icon(:plus, pat(:new)), url(:store_products, :new) %></li>
  <li class="active"> <%= link_to tag_icon(:edit, pat(:edit)), url(:store_products, :edit, :id => @store_product.id) %></li>
</ul>
<div class="tabs-content">
  <% form_for :store_product, url(:store_products, :update, :id => @store_product.id), :method => :put, :class => 'form-horizontal' do |f| %>
    <%= partial 'store_products/form', :locals => { :f => f } %>
  <% end %>
</div>
